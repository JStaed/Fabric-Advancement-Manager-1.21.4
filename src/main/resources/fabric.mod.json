{
	"schemaVersion": 1,
	"id": "advancementmanager",
	"version": "${version}",
	"name": "Advancement Manager",
	"description": "Create custom advancements and/or override vanilla advancements",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://forgedinteractive.net",
		"sources": "https://github.com/jstaed/Fabric-Advancement-Manager-1.21.4"
	},
	"license": "MIT",
	"icon": "assets/advancementmanager/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.forgedinteractive.advancementmanager.AdvancementManager"
		],
		"fabric-datagen": [
			"net.forgedinteractive.advancementmanager.AdvancementManagerDataGenerator"
		]
	},
	"mixins": [
		"advancementmanager.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}